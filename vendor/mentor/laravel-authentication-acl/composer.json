{
  "name": "mentor/laravel-authentication-acl",
  "description": "Laravel authentication and ACL package with a cool admin panel",
  "license": "MIT",
  "keywords": ["acl", "authentication", "signup", "admin panel", "profile", "laravel"],
  "authors": [
    {
      "name": "mentor",
      "email": "mentor@mentorbeschi.com",
      "homepage": "http://www.mentorbeschi.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git://github.com/orchestral/phpseclib.git"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "4.2.*",
    "intervention/image": "2.*",
    "cartalyst/sentry": "2.1.*",
    "mentor/laravel-library": "1.2.*",
    "gregwar/captcha": "1.0.11"
  },
  "require-dev": {
    "mockery/mockery": "0.9.0",
    "orchestra/testbench": "2.2.*",
    "doctrine/dbal": "v2.4.2",
    "fzaninotto/faker": "v1.3.0",
    "phpunit/phpunit": "4.2.0",
    "codeception/codeception": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Mentordeveloper\\Authentication\\": "src/Mentordeveloper/Authentication",
      "Mentordeveloper\\Authentication\\Tests\\Unit\\": "tests/phpunit",
      "Mentordeveloper\\Authentication\\Codeception\\": "tests/"
    },
    "classmap": [
      "src/controllers",
      "src/commands",
      "src/seeds"
    ]
  },
  "minimum-stability": "stable"
}